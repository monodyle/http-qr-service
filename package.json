  "dependencies": {
    "@resvg/resvg-js": "2.6.0",
    "@vercel/og": "0.6.2",
    "easyqrcodejs-nodejs": "4.5.1",
    "satori": "0.10.11",
    "satori-html": "0.3.2"
  },
  "devDependencies": {
    "@swc-node/register": "1.6.8",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.104",
    "@types/node": "20.11.5",
    "chokidar": "3.5.3",
    "npm-run-all": "4.1.5",
    "typescript": "5.3.3"
  }
}
